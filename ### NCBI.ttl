### SSH DATASET 

ncbi:dataset-shh        a                       qb:DataSet ;
                        dct:title               "SHH sonic hedgehog signaling molecule [ Homo sapiens (human) ]"@en ;
                        rdfs:label              "SHH sonic hedgehog signaling molecule [ Homo sapiens (human) ]"@en ;
                        rdfs:comment            "Dataset about SHH sonic hedgehog signaling molecule (Homo Sapiens)"@en ;
                        rdfs:description        "Dataset about SHH sonic hedgehog signaling molecule (Homo Sapiens)"@en ;
                        dct:publisher           ncbi:ncbi ;
                        dct:subject             dbpedia-owl:Gene, biotop:Gene, uniprot:Gene, biopaxa:Gene,
                                                wd:Q7187, wd:Q14860072, wd:Q15978631 ;
                        qb:structure            ncbi:dsd-dataset-shh .


### SHH DATASET STRUCTURE

ncbi:dsd-dataset-shh    a                       qb:DataStructureDefinition ;
                        qb:component
                                [qb:dimension   ncbi:geneId,
                                                ncbi:symbol,
                                                ncbi:description,
                                                ncbi:taxname,
                                                ncbi:commonName,
                                                ncbi:taxId,
                                                ncbi:genomicRanges,
                                                ncbi:orientation,
                                                ncbi:location,
                                                ncbi:type,
                                                ncbi:transcriptAccession,
                                                ncbi:transcriptName,
                                                ncbi:transcriptLength,
                                                ncbi:cdsCoordinates,
                                                ncbi:proteinAccession,
                                                ncbi:isoformName,
                                                ncbi:proteinLength,
                                                ncbi:proteinName
                                ] .


### NCBI ORGANIZATION

ncbi:ncbi               a                       org:Organization, foaf:Agent ;
                        rdfs:label              "National Center for Biotechnology Information"@en.


### NCBI CODE LIST

ncbi:GeneDescriptor     a                       skos:ConceptScheme, owl:Ontology ;
                        skos:prefLabel          "Code list for NCBI (Gene) - codelist scheme"@en ;
                        rdfs:label              "Code list for NCBI (Gene) - codelist scheme"@en ;
                        skos:notation           "CL_GENE";
                        skos:note               "This code list provides information about the gene."@en ;
                        rdfs:description        "This code list provides information about the gene."@en .

ncbi:TranscriptDescriptor       a               skos:ConceptScheme, owl:Ontology ;
                        skos:prefLabel          "Code list for NCBI (Gene - Transcript) - codelist scheme"@en ;
                        rdfs:label              "Code list for NCBI (Gene - Transcript) - codelist scheme"@en ;
                        skos:notation           "CL_TRANSCRIPT";
                        skos:note               "This code list provides information about the transcript(s)."@en ;
                        rdfs:description        "This code list provides information about the transcript(s)."@en .

ncbi:ProteinDescriptor  a                       skos:ConceptScheme, owl:Ontology ;
                        skos:prefLabel          "Code list for NCBI (Protein) - codelist scheme"@en ;
                        skos:label              "Code list for NCBI (Protein) - codelist scheme"@en ;
                        skos:notation           "CL_PROTEIN";
                        skos:note               "This code list provides information about the protein(s)."@en ;
                        rdfs:description        "This code list provides information about the protein(s)."@en .


### GENE DESCRIPTOR GENE TYPES

ncbi:GeneDescriptorType a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Gene Type Enumeration"@en ;
                        skos:definition         "Enumeration of gene types"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:gdt-unknown        a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "UNKNOWN" ;
                        skos:notation           "0"^^xsd:integer .

ncbi:gdt-tRNA           a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "tRNA" ;
                        skos:notation           "1"^^xsd:integer .       

ncbi:gdt-rRNA           a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "rRNA" ;
                        skos:notation           "2"^^xsd:integer .       

ncbi:gdt-snRNA          a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "snRNA" ;
                        skos:notation           "3"^^xsd:integer .       

ncbi:gdt-scRNA          a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "scRNA" ;
                        skos:notation           "4"^^xsd:integer .       

ncbi:gdt-snoRNA         a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "snoRNA" ;
                        skos:notation           "5"^^xsd:integer .       

ncbi:gdt-PROTEINCODING  a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "PROTEIN_CODING" ;
                        skos:notation           "6"^^xsd:integer .       

ncbi:gdt-PSEUDO         a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "PSEUDO" ;
                        skos:note               "these will have NG or NR"@en ;
                        skos:notation           "7"^^xsd:integer .       

ncbi:gdt-TRANSPOSON     a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "TRANSPOSON" ;
                        skos:notation           "8"^^xsd:integer .       

ncbi:gdt-miscRNA        a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "miscRNA" ;
                        skos:notation           "9"^^xsd:integer .       

ncbi:gdt-ncRNA          a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "ncRNA" ;
                        skos:notation           "10"^^xsd:integer .       

ncbi:gdt-BIOLOGICALREGION          a            skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "BIOLOGICALREGION" ;
                        skos:note               "these will have NG"@en ;
                        skos:notation           "11"^^xsd:integer .       

ncbi:gdt-OTHER          a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:GeneDescriptorType ;
                        skos:prefLabel          "OTHER" ;
                        skos:notation           "255"^^xsd:integer .    


### GENE ORIENTATION TYPES

ncbi:OrientationType    a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Orientation Enumeration"@en ;
                        skos:definition         "Enumeration of gene orientation"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:ot-none            a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:OrientationType ;
                        skos:prefLabel          "none" ;
                        skos:notation           "0"^^xsd:integer .

ncbi:ot-plus            a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:OrientationType ;
                        skos:prefLabel          "plus" ;
                        skos:notation           "1"^^xsd:integer .

ncbi:ot-minus           a                       skos:Concept, owl:Class ;
                        rdfs:subClassOf         ncbi:OrientationType ;
                        skos:prefLabel          "minus" ;
                        skos:notation           "2"^^xsd:integer .


### NCBI CONCEPT CLASSES

ncbi:Gene               a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Gene"@en ;
                        skos:altLabel           "gene"@en ;
                        skos:definition         "Concept of a gene"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:Organism           a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Organism"@en ;
                        skos:altLabel           "organism"@en ;
                        skos:definition         "Concept of an organism"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:GeneTranscript     a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Transcript"@en ;
                        skos:altLabel           "transcript"@en ;
                        skos:definition         "Concept of a transcript associated with a gene"@en ;
                        skos:inScheme           ncbi:TranscriptDescriptor .

ncbi:Protein            a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Protein"@en ;
                        skos:altLabel           "protein"@en ;
                        skos:definition         "Concept of a protein associated with a gene or transcript"@en ;
                        skos:inScheme           ncbi:ProteinDescriptor .

ncbi:Isoform            a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Isoform"@en ;
                        skos:altLabel           "isoform"@en ;
                        skos:definition         "Concept of an isoform(s) of the protein associated with a transcript(s)"@en ;
                        skos:inScheme           ncbi:ProteinDescriptor .

ncbi:GeneID             a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "NCBI GeneID"@en ;
                        skos:altLabel           "gene-id"@en ;
                        skos:definition         "NCBI Gene ID"@en ;
                        skos:example            "2778" ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:Symbol             a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Symbol"@en ;
                        skos:altLabel           "symbol"@en ;
                        skos:definition         "gene symbol"@en ;
                        skos:example            "GNAS" ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:Description        a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Description"@en ;
                        skos:altLabel           "description"@en ;
                        skos:definition         "gene name"@en ;
                        skos:example            "GNAS complex locus"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:TaxonomicName      a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Taxonomic Name"@en ;
                        skos:altLabel           "tax-name"@en ;
                        skos:definition         "Taxonomic name of the organism"@en ;
                        skos:example            "Homo sapiens"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:CommonName         a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Common Name"@en ;
                        skos:altLabel           "common-name"@en ;
                        skos:definition         "Common name of the organism"@en ;
                        skos:example            "human"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:TaxonomicID        a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Taxonomic ID"@en ;
                        skos:altLabel           "tax-id"@en ;
                        skos:definition         "NCBI Taxonomy ID for the organism"@en ;
                        skos:example            "9606" ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:GenomicRange       a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Genomic Range"@en ;
                        skos:altLabel           "genomic-range-repeated"@en ;
                        skos:example            "NC_000007.14:155799980-155812463;NC_060931.1:156983127-156995607" ;
                        skos:inScheme           ncbi:GeneDescriptor . 

ncbi:Orientation        a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Orientation"@en ;
                        skos:altLabel           "orientation"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:LocationOnChromosome       a               skos:Concept, owl:Class ;
                        skos:prefLabel          "Gene Location on Chromosome"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:GeneType           a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Gene Type"@en ;
                        skos:altLabel           "gene-type"@en ;
                        skos:inScheme           ncbi:GeneDescriptor .

ncbi:TranscriptAccession         a              skos:Concept, owl:Class ;
                        skos:prefLabel          "Transcript Accession"@en ;
                        skos:altLabel           "transcript-"@en ;
                        skos:definition         "RefSeq coding and non-coding transcript accessions"@en ;
                        skos:example            "NR_132319.2" ;
                        skos:inScheme           ncbi:TranscriptDescriptor .

ncbi:TranscriptName     a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Transcript Name"@en ;
                        skos:altLabel           "name"@en ;
                        skos:definition         "RefSeq transcript name"@en ;
                        skos:example            "transcript variant 12"@en ;
                        skos:inScheme           ncbi:TranscriptDescriptor .

ncbi:TranscriptLength   a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Transcript Length"@en ;
                        skos:altLabel           "length"@en ;
                        skos:definition         "RefSeq transcript length in nucleotides"@en ;
                        skos:example            "3180" ;
                        skos:inScheme           ncbi:TranscriptDescriptor .

ncbi:CdsCoordinates     a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Coding Region (CDS) Coordinates"@en ;
                        skos:altLabel           "cds coords"@en ;
                        skos:definition         "Coordinates of the coding region (cds)"@en ;
                        skos:example            "342-1730" ;
                        skos:inScheme           ncbi:TranscriptDescriptor .

ncbi:CDS                a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "CDS"@en ;
                        skos:altLabel           "cds-"@en ;
                        skos:definition         "Coordinates of the coding region (cds) add accession number of the transcript"@en ;
                        skos:example            "NM_000193.4:342-1730" ;
                        skos:inScheme           ncbi:TranscriptDescriptor .

ncbi:ProteinAccession   a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Protein Accession"@en ;
                        skos:altLabel           "protein accession"@en ;
                        skos:definition         "RefSeq protein accession with version"@en ;
                        skos:example            "NP_001296812.1" ;
                        skos:inScheme           ncbi:ProteinDescriptor .

ncbi:IsoformName        a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Isoform Name"@en ;
                        skos:altLabel           "isoform name"@en ;
                        skos:definition         "Protein isoform name"@en ;
                        skos:example            "isoform Alex" ;
                        skos:inScheme           ncbi:ProteinDescriptor .

ncbi:ProteinLength      a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Protein Length"@en ;
                        skos:altLabel           "protein length"@en ;
                        skos:definition         "Protein length in amino acids"@en ;
                        skos:example            "626" ;
                        skos:inScheme           ncbi:ProteinDescriptor .

ncbi:ProteinName        a                       skos:Concept, owl:Class ;
                        skos:prefLabel          "Protein Name"@en ;
                        skos:altLabel           "protein name"@en ;
                        skos:definition         "Protein name"@en ;
                        skos:example            "protein Alex" ;
                        skos:inScheme           ncbi:ProteinDescriptor .


### SHH DIMENSIONS

ncbi:isGene             a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "is gene"@en ;
                        qb:concept              ncbi:Gene .

ncbi:hasOrganism        a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "organism of the gene"@en ;
                        qb:concept              ncbi:Organism .

ncbi:hasTranscript      a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "transcript(s) associated with the gene"@en ;
                        qb:concept              ncbi:GeneTranscript .

ncbi:hasProtein         a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "protein(s) associated with the transcript(s)"@en ;
                        qb:concept              ncbi:Protein . 

ncbi:hasIsoform         a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "isoform(s) of the protein associated with the transcript(s)"@en ;
                        owl:propertyChainAxiom  (ncbi:hasTranscript ncbi:hasProtein) ;
                        qb:concept              ncbi:Isoform . 

ncbi:hasGeneType        a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "has gene type"@en ;
                        rdfs:range              ncbi:GeneDescriptorType ;
                        qb:concept              ncbi:GeneDescriptorType . 

ncbi:geneId             a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "gene_id"@en ;
                        rdfs:range              xsd:long ;
                        owl:propertyChainAxiom  (ncbi:isGene dcterms:identifier) ;
                        qb:concept              ncbi:GeneID .

ncbi:symbol             a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "gene_symbol"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:isGene dbpedia-owl:symbol) ;
                        qb:concept              ncbi:Symbol .

ncbi:description        a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "description"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:isGene rdfs:description) ;
                        qb:concept              ncbi:Description .  

ncbi:taxname            a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "scientific_name"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:hasOrganism biol:name) ;
                        qb:concept              ncbi:TaxonomicName .    

ncbi:commonName         a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "common_name"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:hasOrganism biol:commonName) ;
                        qb:concept              ncbi:CommonName .                 
                                    
ncbi:taxId              a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "tax_id"@en ;
                        rdfs:range              xsd:long ;
                        owl:propertyChainAxiom  (ncbi:hasOrganism dcterms:identifier) ;
                        qb:concept              ncbi:TaxonomicID .

ncbi:genomicRanges      a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "genomic_range"@en ;
                        rdfs:range              xsd:string ;
                        qb:concept              ncbi:GenomicRange .

ncbi:orientation        a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "orientation"@en ;
                        rdfs:range              ncbi:OrientationType ;
                        owl:propertyChainAxiom  (ncbi:isGene uniprot:orientation) ;
                        qb:concept              ncbi:Orientation .

ncbi:location           a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "Chromosomal Location"@en ;
                        rdfs:range              xsd:integer ;
                        owl:propertyChainAxiom  (ncbi:isGene dbpedia-owl:onChromosome) ;
                        qb:concept              ncbi:LocationOnChromosome .

ncbi:type               a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "gene_type"@en ;
                        rdfs:range              ncbi:GeneDescriptorType ;
                        owl:propertyChainAxiom  (ncbi:isGene biol:hasGeneType) ;
                        qb:concept              ncbi:GeneType .

ncbi:transcriptAccession         a              rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "transcript_accession"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:hasTranscript dicom:AccessionNumber) ;
                        qb:concept              ncbi:TranscriptAccession .

ncbi:transcriptName     a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ; 
                        rdfs:label              "transcript_name"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:hasTranscript dbpedia-owl:name) ;
                        qb:concept              ncbi:TranscriptName .

ncbi:transcriptLength   a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "transcript_length"@en ;
                        rdfs:range              xsd:integer ;
                        owl:propertyChainAxiom  (ncbi:hasTranscript uniprot:length) ;
                        qb:concept              ncbi:TranscriptLength .

ncbi:cdsCoordinates     a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "Coding region (cds) coordinates"@en ;
                        rdfs:range              xsd:string ;
                        qb:concept              ncbi:CdsCoordinates .

ncbi:cds                a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "transcript_cds_coords"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:hasTranscript ncbi:cdsCoordinates) ;
                        qb:concept              ncbi:CDS .

ncbi:proteinAccession   a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "protein_accession"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:hasProtein dicom:AccessionNumber) ;
                        qb:concept              ncbi:ProteinAccession .

ncbi:isoformName        a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "isoform_name"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:hasIsoform dbpedia-owl:name) ;
                        qb:concept              ncbi:IsoformName .

ncbi:proteinLength      a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "protein_length"@en ;
                        rdfs:range              xsd:integer ;
                        owl:propertyChainAxiom  (ncbi:hasProtein uniprot:length) ;
                        qb:concept              ncbi:ProteinLength .

ncbi:proteinName        a                       rdf:Property, owl:ObjectProperty, qb:DimensionProperty ;
                        rdfs:label              "protein_name"@en ;
                        rdfs:range              xsd:string ;
                        owl:propertyChainAxiom  (ncbi:hasProtein dbpedia-owl:name) ;
                        qb:concept              ncbi:ProteinName .
