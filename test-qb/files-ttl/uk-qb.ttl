# -- Data Set --------------------------------------------

<http://www.uk-open-data.org/example-aggregated-dataset>        a                               qb:DataSet ;
                                                                dct:subject                     sdmx-subject:1 ,        # demographic and social statistics
                                                                                                sdmx-subject:3.2 ,      # regional and small area statistics
                                                                                                sdmx-subject:1.4 ;      # health
                                                                dsv:summaryStatistics           [
                                                                    dsv:numberOfRows                "962760"^^xsd:integer ;
                                                                    dsv:numberOfColumns             "10"^^xsd:integer ;
                                                                    dsv:datasetCompleteness         "0.98"^^xsd:float ;
                                                                    dsv:missingValuesFormat         "NA"^^xsd:string
                                                                ] ;
                                                                qb:structure                    eg:dsd-uk ;                      
                                                                sdmx-attribute:statUnit         ddi-au:Group ;
                                                                sdmx-attribute:unitMeasure      <http://dbpedia.org/resource/Year> .


# -- Data structure definition ----------------------------

eg:dsd-uk                                                       a                               qb:DataStructureDefinition ;
                                                                qb:component                    [
                                                                    qb:dimension                    <http://www.uk-open-data.org/example-aggregated-dataset/column/year> ,
                                                                                                    <http://www.uk-open-data.org/example-aggregated-dataset/column/rgn20cd> ,
                                                                                                    <http://www.uk-open-data.org/example-aggregated-dataset/column/rgn20nm> ,
