###  https://w3id.org/dsv-ontology#AggregatedDataset
:AggregatedDataset rdf:type owl:Class ;
                   rdfs:subClassOf :Dataset ;
                   rdfs:comment "Set of data that has been aggregated over an individual - or unaggregated - set of data." ;
                   rdfs:label "Aggregated Dataset" .


###  https://w3id.org/dsv-ontology#Dataset
:Dataset rdf:type owl:Class ;
         rdfs:subClassOf <http://purl.org/linked-data/cube#DataSet> ,
                         <http://www.w3.org/ns/csvw#Table> ;
         rdfs:comment "Set of data." ;
         rdfs:label "Dataset" .


###  https://w3id.org/dsv-ontology#DatasetComponent
:DatasetComponent rdf:type owl:Class ;
                  rdfs:subClassOf <http://purl.org/linked-data/cube#ComponentSpecification> ;
                  rdfs:comment "Set of individual component of a dataset." ;
                  rdfs:label "Dataset Component" .


###  https://w3id.org/dsv-ontology#DatasetSchema
:DatasetSchema rdf:type owl:Class ;
               rdfs:subClassOf <http://purl.org/linked-data/cube#DataStructureDefinition> ,
                               <http://www.w3.org/ns/csvw#TableSchema> ;
               rdfs:comment "The structural schema of a dataset." ;
               rdfs:label "Dataset Schema" .


###  https://w3id.org/dsv-ontology#IndividualDataset
:IndividualDataset rdf:type owl:Class ;
                   rdfs:subClassOf :Dataset ;
                   rdfs:comment "Set of data that can be aggregated over. Usually representing the raw data. " ;
                   rdfs:label "Individual Dataset" .


###  https://w3id.org/dsv-ontology#IntervalStatisticalDataType
:IntervalStatisticalDataType rdf:type owl:Class ;
                             rdfs:subClassOf :QuantitativeStatisticalDataType ;
                             rdfs:comment "Statistical data type representing ordered data measured over a numerical scale with equal distance between units." ;
                             rdfs:label "Interval Statistical Data Type" .


###  https://w3id.org/dsv-ontology#NominalStatisticalDataType
:NominalStatisticalDataType rdf:type owl:Class ;
                            rdfs:subClassOf :QualitativeStatisticalDataType ;
                            rdfs:comment "Statistical data type representing discrete units that cannot be ordered or measured." ;
                            rdfs:label "Nominal Statistical Data Type" .


###  https://w3id.org/dsv-ontology#OrdinalStatisticalDataType
:OrdinalStatisticalDataType rdf:type owl:Class ;
                            rdfs:subClassOf :QualitativeStatisticalDataType ;
                            rdfs:comment "Statistical data type representing discrete units that can be ordered or measured." ;
                            rdfs:label "Ordinal Statistical Data Type" .


###  https://w3id.org/dsv-ontology#QualitativeStatisticalDataType
:QualitativeStatisticalDataType rdf:type owl:Class ;
                                rdfs:subClassOf :StatisticalDataType ;
                                rdfs:comment "Set of statistical data types can be observed and - or - categorised." ;
                                rdfs:label "Qualitative Statistical Data Type" .


###  https://w3id.org/dsv-ontology#QuantitativeStatisticalDataType
:QuantitativeStatisticalDataType rdf:type owl:Class ;
                                 rdfs:subClassOf :StatisticalDataType ;
                                 rdfs:comment "Set of statistical data types can be measured and quantified." ;
                                 rdfs:label "Quantitative Statistical Data Type" .


###  https://w3id.org/dsv-ontology#RatioStatisticalDataType
:RatioStatisticalDataType rdf:type owl:Class ;
                          rdfs:subClassOf :QuantitativeStatisticalDataType ;
                          rdfs:comment "Statistical data type representing ordered data with equal distance between units, where negative values are not allowed." ;
                          rdfs:label "Ratio Statistical Data Type" .


###  https://w3id.org/dsv-ontology#StatisticalDataType
:StatisticalDataType rdf:type owl:Class ;
                     rdfs:comment "Set of statistical data types." ;
                     rdfs:label "Statistical Data Type" .


###  https://w3id.org/dsv-ontology#SummaryStatistics
:SummaryStatistics rdf:type owl:Class ;
                   rdfs:comment "Set of summary statistics about the dataset or column." ;
                   rdfs:label "Summary Statistics" .


###  https://w3id.org/dsv-ontology#Variable
:Variable rdf:type owl:Class ;
