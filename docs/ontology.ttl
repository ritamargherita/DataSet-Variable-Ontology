###  https://w3id.org/dsv-ontology#componentProperty
:componentProperty rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://purl.org/linked-data/cube#dimension> ,
                                      <http://www.w3.org/ns/csvw#propertyUrl> ;
                   rdfs:domain :DatasetComponent ;
                   rdfs:range rdf:Property ;
                   rdfs:comment "Property linking a component to a property." ;
                   rdfs:label "component property" .


###  https://w3id.org/dsv-ontology#datasetComponent
:datasetComponent rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://purl.org/linked-data/cube#component> ;
                  rdfs:domain :DatasetSchema ;
                  rdfs:range :DatasetComponent ;
                  rdfs:comment "Property linking the dataset schema to its components." ;
                  rdfs:label "dataset component" .


###  https://w3id.org/dsv-ontology#datasetSchema
:datasetSchema rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://purl.org/linked-data/cube#structure> ,
                                  <http://www.w3.org/ns/csvw#tableSchema> ;
               rdfs:domain :Dataset ;
               rdfs:range :DatasetSchema ;
               rdfs:comment "Property linking the dataset to its schema." ;
               rdfs:label "dataset schema" .


###  https://w3id.org/dsv-ontology#linksToVariable
:linksToVariable rdf:type owl:ObjectProperty ;
                 rdfs:domain rdf:Property ;
                 rdfs:range :Variable ;
                 rdfs:comment "Property linking a property of a component to a variable." ;
                 rdfs:label "links to variable" .


###  https://w3id.org/dsv-ontology#refersToVariable
:refersToVariable rdf:type owl:ObjectProperty ;
                  rdfs:domain :DatasetComponent ;
                  rdfs:range :Variable ;
                  rdfs:comment "Property linking a component to a variable." ;
                  rdfs:label "refers to variable" .


###  https://w3id.org/dsv-ontology#rowsRepresentInstancesOf
:rowsRepresentInstancesOf rdf:type owl:ObjectProperty ;
                          rdfs:domain :Dataset ;
                          rdfs:range owl:Class ;
                          rdfs:comment "Property linking a dataset to the conceptual representation of its rows." ;
                          rdfs:label "rows represent instances of" .


###  https://w3id.org/dsv-ontology#statisticalDataType
:statisticalDataType rdf:type owl:ObjectProperty ;
                     rdfs:domain :SummaryStatistics ;
                     rdfs:range :StatisticalDataType ;
                     rdfs:comment "Property linking a component to its statistical data type." ;
                     rdfs:label "statistical data type" .


###  https://w3id.org/dsv-ontology#subVariableOf
:subVariableOf rdf:type owl:ObjectProperty ;
               rdfs:domain :Variable ;
               rdfs:range :Variable ;
               rdfs:comment "Property linking a variable to its sub-variable." ;
               rdfs:label "sub-variable of" .


###  https://w3id.org/dsv-ontology#summaryStatistics
:summaryStatistics rdf:type owl:ObjectProperty ;
                   rdfs:domain :Dataset ,
                               :DatasetComponent ;
                   rdfs:range :SummaryStatistics ;
                   rdfs:comment "Property linking a dataset or a component to their summary statistics." ;
                   rdfs:label "summary statistics" .


