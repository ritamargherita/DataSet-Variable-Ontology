@prefix ex:             <https://www.example.org> . 
@prefix var:            <https://www.variable-thesaurus.org/variable#> .
@prefix varProp:        <https://www.variable-thesaurus.org/property#> .
@prefix i-adopt:        <https://w3id.org/iadopt/ont/> .
@prefix sur:            <https://w3id.org/survey-ontology#> .
@prefix skos:           <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema> .
@prefix xsd:            <https://www.w3.org/2001/XMLSchema#> .
@prefix dbpedia-owl:    <http://dbpedia.org/ontology/> .
@prefix wikidata:       <https://www.wikidata.org/wiki/Property:> .
@prefix geo:            <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix ex:             <http://example.org/> .

# -- DATASET  -----------------------------

<http://www.example.org/datasets/csse>          a                       csvw:Table ;
                                                csvw:tableSchema    [
                                                    csvw:columns            ex:column1 ,
                                                                            ex:column2 ,
                                                                            ex:column3 ,
                                                                            ex:column4 ,
                                                                            ex:column5 ,
                                                                            ex:column6
                                                ] ;
                                                csvw:row                ex:row1 , ex:row2 .

# -- DATA  --------------------------------

ex:row1     {

    ex:cell1-1                  a                           csvw:Cell ;
                                wikidata:P131               "Western Australia" ;  
                                #varProp:hasProvinceState    "Western Australia" ;
                                csvw:datatype               xsd:string .

    ex:cell1-2                  a                           csvw:Cell ;
                                varProp:hasCountryRegion    "Australia" ;
                                csvw:datatype               xsd:string .

}          

ex:row2     {
    
    ex:cell2-1                  a                           csvw:Cell ;
                                varProp:hasProvinceState    "Ontario" ;
                                csvw:datatype               xsd:string .
    
    ex:cell2-2                  a                           csvw:Cell ;
                                varProp:hasCountryRegion    "Canada" ;
                                csvw:datatype               xsd:String .
                                
}
# -- ROW 1  -------------------------------

ex:row1                                         a                       csvw:Row ;
                                                csvw:rownum             1 .          

# -- ROW 2  -------------------------------

ex:row2                                         a                       csvw:Row ;
                                                csvw:rownum             2 . 

# -- COLUMN 1  ----------------------------

ex:column1                                      a                       csvw:Column ;
                                                csvw:name               "Province/State" ;
                                                csvw:datatype           xsd:string ;
                                                csvw:propertyUrl        varProp:hasProvinceState .

varProp:hasProvinceState                        a                       i-adopt:Property, rdfs:Property, owl:DataProperty ;
                                                skos:prefLabel          "Province/State" ;
                                                owl:equivalentProperty  dbpedia-owl:province, wikidata:P131 ;
                                                rdfs:range              xsd:string .


# -- COLUMN 2  ----------------------------

ex:column2                                      a                       csvw:Column ;
                                                csvw:name               "Country/Region" ;
                                                csvw:datatype           xsd:string ;
                                                csvw:propertyUrl        varProp:hasCountryRegion .

varProp:hasCountryRegion                        a                       i-adopt:Property, rdfs:Property, owl:DataProperty ;
                                                skos:prefLabel          "Country/Region" ;
                                                owl:equivalentProperty  dbpedia-owl:region ;
                                                rdfs:range              xsd:string .

# -- COLUMN 3  ----------------------------

ex:column3                                      a                       csvw:Column ;
                                                csvw:name               "Lat" ;
                                                csvw:datatype           xsd:float ;
                                                csvw:propertyUrl        varProp:hasLatitude .

varProp:hasLatitude                             a                       i-adopt:Property, rdfs:Property, owl:DataProperty ;
                                                skos:prefLabel          "Latitude" ;
                                                owl:equivalentProperty  geo:lat ;
                                                rdfs:range              xsd:float .

# -- COLUMN 4  ----------------------------

ex:column4                                      a                       csvw:Column ;
                                                csvw:name               "Long" ;
                                                csvw:datatype           xsd:float ;
                                                csvw:propertyUrl        varProp:hasLongitude .

varProp:hasLongitude                            a                       i-adopt:Property, rdfs:Property, owl:DataProperty ;
                                                skos:prefLabel          "Longitude" ;
                                                owl:equivalentProperty  geo:long ;
                                                rdfs:range              xsd:float .

# -- COLUMN 5  ----------------------------

ex:column5                                      a                       csvw:Column ;
                                                csvw:name               "01/22/20" ;
                                                csvw:datatype           xsd:float ;
                                                csvw:propertyUrl        varProp:hasNumberOfCovidCasesOn012220 .

varProp:hasNumberOfCovidCasesOn012220           a                       i-adopt:Property, rdfs:Property, owl:DataProperty ;
                                                skos:prefLabel          "Number of COVID cases on 01/22/20" ;
                                                owl:subPropertyOf       varProp:hasNumberOfCovidCasesDaily ;
                                                ### ADD DATE PROPERTY
                                                rdfs:range              xsd:float .

varProp:hasNumberOfCovidCasesDaily              a                       i-adopt:Property, rdfs:Property, owl:DataProperty ;
                                                skos:prefLabel          "Number of COVID cases on a specific date" ;
                                                ### ADD PROPERTY FOR DAILY FREQUENCY
                                                rdfs:range              xsd:float .




varProp:hasDate                                 rdfs:range          xsd:Date .



# -- COLUMN 6  ----------------------------

ex:column6                                      a                       csvw:Column ;
                                                csvw:name               "01/22/20" ;
                                                csvw:datatype           xsd:string ;
                                                csvw:propertyUrl        varProp:hasLongitude .

varProp:hasLongitude                            a                       i-adopt:Property, rdfs:Property, owl:DataProperty ;
                                                skos:prefLabel          "Longitude" ;
                                                owl:equivalentProperty  geo:long ;
                                                rdfs:range              xsd:float .

# -- VARIABLES  ----------------------------

var:Location                                    a                       i-adopt:Variable, sur:ObservableVariable ;
                                                i-adopt:hasProperty     varProp:hasCountryRegion,
                                                                        varProp:hasProvinceState ;
                                                owl:equivalentProperty  wikidata:P276 , # "location" instance of property
                                                                        wikidata:Q2221906 , # "geographic location" instance of property
                                                                        wikidata:Q27096213 , # "geographic entity" subclass of entity
                                                                        wikidata:Q17334923; # "physical location" instance of physical property
                                                skos:related            wikidata:P276 , # "location" instance of property
                                                                        wikidata:Q2221906 , # "geographic location" instance of property
                                                                        wikidata:Q27096213 , # "geographic entity" subclass of entity
                                                                        wikidata:Q17334923 . # "physical location" instance of physical property

var:NumberCovidCases                            a                       i-adopt:Variable, sur:ObservableVariable ;
                                                i-adopt:hasProperty     varProp:hasNumberOfCovidCasesDaily .

