<dataset/cbs/d0b01e410801f5de6>         a                           csvw:Table , 
                                                                    schema:Dataset ;
                                        schema:name                 "Uitgenodigde Vluchtelingen JJJJ"@nl ,
                                                                    "Invited Refugees YYYY"@en ;
                                        schema:description          "Deze tabel bevat informatie over het jaarlijks aantal door Nederland uitgenodigde vluchtelingen. Hiervoor komen personen in aanmerking die vaak al jaren in vluchtelingenkampen leven en niet naar het land van herkomst kunnen terugkeren. Uitgenodigde vluchtelingen worden door de IND op voordracht van de vluchtelingenorganisatie van de Verenigde Naties (UNHCR) geselecteerd. De vluchtelingen komen naar Nederland in het kader van het hervestigingsprogramma van de UNHCR. Uitgenodigde vluchtelingen hebben recht op een verblijfsvergunning en hoeven de Nederlandse asielprocedure niet te doorlopen. Daarom worden ze ook niet in de cijfers over asielaanvragen meegeteld."@nl ,
                                                                    "This table contains information about the annual number of refugees invited by the Netherlands. This applies to people who have often been living in refugee camps for years and who cannot return to their country of origin. Invited refugees are selected by the IND on the recommendation of the United Nations Refugee Organization (UNHCR). The refugees come to the Netherlands as part of the UNHCR resettlement programme. Invited refugees are entitled to a residence permit and do not have to go through the Dutch asylum procedure. That is why they are not included in the figures on asylum applications."@en ;
                                        # I don't think cbsSchema:dataDesignGroup is needed to be added in this case
                                        cbsSchema:dataDesignGroup   "Sociaaleconomische en ruimtelijke statistieken/Rechtsbescherming en veiligheid/Asiel en verblijf"@nl ,
                                                                    "Socio-economic and spatial statistics/Legal protection and security/Asylum and residence"@en ;
                                        csvw:tableSchema            [
                                            csvw:columns                    <column/local/totaal> ,
                                                                            <column/local/jaar> ,
                                                                            <column/local/nationaliteit> ,
                                                                            <column/local/leeftijd> ,
                                                                            <column/local/geslacht> ;
                                            eg:rowsRepresentInstanceOf      <event/refugee/invitation>
                                        ] ;
                                        schema:creator              [
                                            a                               schema:GovernmentOrganization ;
                                            schema:name                     "Centraal Bureau voor de Statistiek" ;
                                            schema:url                      "https://www.cbs.nl" 
                                        ] ;
                                        schema:populationType       "Uitgenodigde vluchtelingen"@nl ,
                                                                    foaf:Person ,
                                                                    wd:Q131572 ; # refugee
                                        csvw:datatype               "Dimensioneel"@nl ; # note sure if this is allowed with csvw:datatype
                                        schema:measurementTechnique "Uitgenodigde vluchtelingen hoeven de Nederlandse asielprocedure niet te doorlopen en worden daarom niet meegeteld in de asielcijfers. Dit geldt ook voor de verblijfsvergunningen die worden verleend aan uitgenodigde vluchtelingen."@nl ,
                                                                    "Invited refugees do not have to go through the Dutch asylum procedure and are therefore not included in the asylum figures. This also applies to residence permits issued to invited refugees."@en ;
                                        schema:genre                "Veiligheid en recht"@nl ,
                                                                    "Security and justice"@en ; # or schema:keywords - and this should also be URIs
                                        schema:temporalCoverage     "2008-01-01/.." ; # or schema:contentReferenceTime or schema:datasetTimeInterval
                                        schema:frequency            "Kalenderjaar"@nl ,
                                                                    "Calendar year"@en . # or schema:repeatFrequency
                                        

### ROW REPRESENTATION

<event/refugee/invitation>              a                           sem:Event ;
                                        sem:hasActor                <event/refugee/invitation/actor> ;
                                        #sem:hasTime                 <local/refugee/invitation/event/time> ;
                                        sem:eventType               "CBS Refugee Invitation" .

<event/refugee/invitation/actor>        a                           sem:Actor ;
                                        sem:actorType               foaf:Person ,
                                                                    wd:Q131572 .


### COLUMNS

<column/local/aantal>                   a                           csvw:Column ,
                                                                    schema:Observation ;
                                        csvw:titles                 "Aantal uitgenodigde vluchtelingen"@nl ;
                                        csvw:datatype               xsd:float ;
                                        schema:variableMeasured     cbsVar:AantalUitgenodigdeVluchtelingen . # this property should be of schema:Dataset and not Observation

<column/local/jaar>                     a                           csvw:Column ,
                                                                    schema:Observation ;
                                        csvw:titles                 "Jaar"@nl ;
                                        csvw:datatype               xsd:gYear ;
                                        schema:variableMeasured     cbsVar:JaarVanVluchtelingenuinodiging .

<column/local/nationaliteit>            a                           csvw:Column ,
                                                                    schema:Observation ;
                                        csvw:titles                 "Nationaliteit van de uitgenodigde vluchteling"@nl ;
                                        csvw:datatype               xsd:string ;
                                        schema:variableMeasured     cbsVar:NationaliteitVanDeUitgenodigdeVluchteling .

<column/local/leeftijd>                 a                           csvw:Column ,
                                                                    schema:Observation ;
                                        csvw:titles                 "Leeftijd in groepen van uitgenodigde vluchtelingen"@nl ;
                                        csvw:datatype               xsd:float ;
                                        schema:variableMeasured     cbsVar:LeeftijdInGroepenVanUitgenodigdeVluchtelingen .

<column/local/geslacht>                 a                           csvw:Column ,
                                                                    schema:Observation ;
                                        csvw:titles                 "Geslacht"@nl ;
                                        csvw:datatype               xsd:string ;
                                        schema:variableMeasured     cbsVar:GeslachtVanDeVluchteling .


### I-ADOPT VARIABLES VOCAB

cbsVar:AantalUitgenodigdeVluchtelingen                      a                       schema:Property , 
                                                                                    owl:ObjectProperty ;
                                                            schema:name             "Aantal uitgenodigde vluchtelingen"@nl ,
                                                                                    "Number of invited refugees"@en ;
                                                            schema:measuredValued   xsd:float .

cbsVar:JaarVanVluchtelingenuinodiging                       a                       iop:Variable ;
                                                            iop:hasProperty         time:year ; # this property is used for webpages but might still work here
                                                            iop:hasObjectOfInterest <event/refugee/invitation> ;
                                                            iop:hasContextObject    <event/refugee/invitation/actor> .

cbsVar:NationaliteitVanDeUitgenodigdeVluchteling            a                       iop:Variable ;
                                                            iop:hasProperty         schema:nationality ;
                                                            iop:hasObjectOfInterest <event/refugee/invitation/actor> .

cbsVar:LeeftijdInGroepenVanUitgenodigdeVluchtelingen        a                       iop:Variable ;
                                                            iop:hasProperty         foaf:age ;
                                                            iop:hasObjectOfInterest <event/refugee/invitation/actor> .

cbsVar:GeslachtVanDeVluchteling                             a                       iop:Variable ;
                                                            iop:hasProperty         foaf:gender ;
                                                            iop:hasObjectOfInterest <event/refugee/invitation/actor> .


