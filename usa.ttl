@prefix dsv:            <http://datasetVariableOntology.example/> .
@prefix csvw:           <http://www.w3.org/ns/csvw#> .
@prefix rdfs:           <http://www.w3.org/2000/01/rdf-schema#> .
@prefix var:            <http://example.org/VariablesVocabulary/> .
@prefix xsd:            <http://www.w3.org/2001/XMLSchema#> .


###########################################
# Dataset USA
###########################################

<www.usa-open-data.org/example-aggregated-dataset>          a                               dsv:AggregatedDataset ;
                                                            csvw:url                        "https://catalog.data.gov/dataset/nchs-births-and-general-fertility-rates-united-states" ;
                                                            dsv:rowsRepresentInstancesOf    var:Births ;
                                                            dsv:aggregationMethod   [
                                                                dsv:aggregatedFrom          <www.usa-open-data.org/example-individual-dataset> ;
                                                                dsv:aggregatedBy            <www.usa-open-data.org/example-individual-dataset/column/Year>
                                                            ] ;
                                                            dsv:summaryStatistics   [
                                                                dsv:numberOfRows            "110"^^xsd:integer ;
                                                                dsv:numberOfColumns         "4"^^xsd:integer ;
                                                                dsv:datasetCompleteness     "1.00"^^xsd:float 
                                                            ] ;
                                                            dsv:datasetSchema       [
                                                                dsv:datasetComponent            <www.usa-open-data.org/example-aggregated-dataset/column/Year> ,
                                                                                                <www.usa-open-data.org/example-aggregated-dataset/column/BirthNumber> ,
                                                                                                <www.usa-open-data.org/example-aggregated-dataset/column/GeneralFertilityRate> ,
                                                                                                <www.usa-open-data.org/example-aggregated-dataset/column/CrudeBirthRate> 
                                                            ] .

###########################################
# Dataset USA - Columns
###########################################

<www.usa-open-data.org/example-aggregated-dataset/column/Year>                      a                           dsv:DimensionComponent ,
                                                                                                                csvw:Column ;
                                                                                    rdfs:label                  "Year"@en ;
                                                                                    dsv:componentProperty       <www.usa-open-data.org/properties-dictionary/year> ;
                                                                                    dsv:refersToVariable        var:YearCoverage ;
                                                                                    dsv:hasAggregationMethod    [
                                                                                        dsv:aggregationFunction             "GROUP BY"@en ;
                                                                                        dsv:aggregationFunctionArgument     <www.usa-open-data.org/example-individual-dataset/column/Year> 
                                                                                    ] ;
                                                                                    dsv:summaryStatistics       [
                                                                                        dsv:componentCompleteness           "1.00"^^xsd:float ;
                                                                                        dsv:dataType                        "Numerical"@en ;
                                                                                    ] .

<www.usa-open-data.org/example-aggregated-dataset/column/BirthNumber>               a                           dsv:DimensionComponent ,
                                                                                                                csvw:Column ;
                                                                                    rdfs:label                  "Birth Number"@en ;
                                                                                    dsv:componentProperty       <www.usa-open-data.org/properties-dictionary/birthNumber> ;
                                                                                    dsv:refersToVariable        var:NumberOfBirths ;
                                                                                    dsv:hasAggregationMethod    [
                                                                                        dsv:aggregationFunction             "COUNT DISTINCT"@en ;
                                                                                        dsv:aggregationFunctionArgument     <www.usa-open-data.org/example-individual-dataset/column/new-birth-id> 
                                                                                    ] ;
                                                                                    dsv:summaryStatistics       [
                                                                                        dsv:componentCompleteness           "1.00"^^xsd:float ;
                                                                                        dsv:dataType                        "Numerical"@en ;
                                                                                    ] .

<www.usa-open-data.org/example-aggregated-dataset/column/GeneralFertilityRate>      a                           dsv:DimensionComponent ,
                                                                                                                csvw:Column ;
                                                                                    rdfs:label                  "General Fertility Rate"@en ;
                                                                                    dsv:componentProperty       <www.usa-open-data.org/properties-dictionary/generalFertilityRate> ;
                                                                                    dsv:refersToVariable        var:GeneralFertilityRate ;
                                                                                    dsv:summaryStatistics       [
                                                                                        dsv:componentCompleteness           "1.00"^^xsd:float ;
                                                                                        dsv:dataType                        "Numerical"@en ;
                                                                                    ] .

<www.usa-open-data.org/example-aggregated-dataset/column/CrudeBirthRate>            a                           dsv:DimensionComponent ,
                                                                                                                csvw:Column ;
                                                                                    rdfs:label                  "Crude Birth Rate"@en ;
                                                                                    dsv:componentProperty       <www.usa-open-data.org/properties-dictionary/crudeBirthRate> ;
                                                                                    dsv:refersToVariable        var:CrudeBirthRate ;
                                                                                    dsv:summaryStatistics       [
                                                                                        dsv:componentCompleteness           "1.00"^^xsd:float ;
                                                                                        dsv:dataType                        "Numerical"@en ;
                                                                                    ] .

###########################################
# Dataset USA - Properties
###########################################

<www.usa-open-data.org/properties-dictionary/year>                      a                       rdf:Property ;
                                                                        dsv:linksToVariable     var:YearCoverage ;
                                                                        rdfs:domain             <www.usa-open-data.org/example-aggregated-dataset/rowsRepresentation/nationalStatistics> ;
                                                                        rdfs:range              xsd:gYear .

<www.usa-open-data.org/properties-dictionary/birthNumber>               a                       rdf:Property ;
                                                                        dsv:linksToVariable     var:NumberOfBirths ;
                                                                        rdfs:domain             <www.usa-open-data.org/example-aggregated-dataset/rowsRepresentation/nationalStatistics> ;
                                                                        rdfs:range              xsd:int .

<www.usa-open-data.org/properties-dictionary/generalFertilityRate>      a                       rdf:Property ;
                                                                        dsv:linksToVariable     var:GeneralFertilityRate ;
                                                                        rdfs:domain             <www.usa-open-data.org/example-aggregated-dataset/rowsRepresentation/nationalStatistics> ;
                                                                        rdfs:range              xsd:float .

<www.usa-open-data.org/properties-dictionary/crudeBirthRate>            a                       rdf:Property ;
                                                                        dsv:linksToVariable     var:CrudeBirthRate ;
                                                                        rdfs:domain             <www.usa-open-data.org/example-aggregated-dataset/rowsRepresentation/nationalStatistics> ;
                                                                        rdfs:range              xsd:float .
